@{
    ViewBag.Title = "NIST CVE Data";
}

<h2>NIST CVE Data</h2>

@if (ViewBag.Error != null)
{
    <div class="alert alert-danger">
        @ViewBag.Error
    </div>
}
else
{
    <pre>@ViewBag.CveData</pre>
}
